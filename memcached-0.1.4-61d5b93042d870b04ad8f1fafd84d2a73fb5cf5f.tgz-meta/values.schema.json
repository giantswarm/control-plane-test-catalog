{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "memcached": {
            "type": "object",
            "properties": {
                "architecture": {
                    "type": "string"
                },
                "autoscaling": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "maxReplicas": {
                            "type": "integer"
                        },
                        "minReplicas": {
                            "type": "integer"
                        },
                        "targetCPU": {
                            "type": "integer"
                        },
                        "targetMemory": {
                            "type": "integer"
                        }
                    }
                },
                "commonLabels": {
                    "type": "object",
                    "properties": {
                        "application.giantswarm.io/team": {
                            "type": "string"
                        },
                        "application.kubernetes.io/version": {
                            "type": "string"
                        },
                        "giantswarm.io/managed-by": {
                            "type": "string"
                        },
                        "helm.sh/chart": {
                            "type": "string"
                        }
                    }
                },
                "containerSecurityContext": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
